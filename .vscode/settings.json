// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "typescript.preferences.quoteStyle": "double",
  "files.exclude": {
    "**/node_modules/": true,
    // "**/build/": true,
    // "**/Build/": true,
  },
  "files.watcherExclude": {
    "**/node_modules/": true,
    "**/build/": true,
    "**/Build/": true,
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "CODE_OF_CONDUCT.md",
    "package-lock.json",
    "yarn.lock",
  ],
  "cSpell.words": [
    "argsx",
    "Asynchrony",
    "boilerplating",
    "Chetyrko",
    "circleci",
    "deref",
    "Dmitry",
    "enqueues",
    "gmail",
    "keyof",
    "lcovonly",
    "mpath",
    "multiline",
    "nezaboodka",
    "noindex",
    "nonsubscribing",
    "nothrow",
    "notran",
    "npmjs",
    "npmrc",
    "parens",
    "proto",
    "quotemark",
    "rdbg",
    "reactronic",
    "Reactronic's",
    "restyler",
    "Roadmap",
    "rtti",
    "Seledets",
    "sidebyside",
    "Stateful",
    "subscribeless",
    "Surkov",
    "typeless",
    "unmount",
    "unmounting",
    "untracked",
    "Vadim",
    "verstak",
    "ychetyrko",
    "Yury"
  ]
}
